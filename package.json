{
  "name": "intema-site",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "prepare": "husky"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.61.1",
    "@payloadcms/db-mongodb": "3.61.1",
    "@payloadcms/live-preview-react": "3.61.1",
    "@payloadcms/next": "3.61.1",
    "@payloadcms/payload-cloud": "3.61.1",
    "@payloadcms/plugin-form-builder": "3.61.1",
    "@payloadcms/plugin-nested-docs": "3.61.1",
    "@payloadcms/plugin-redirects": "3.61.1",
    "@payloadcms/plugin-search": "3.61.1",
    "@payloadcms/plugin-seo": "3.61.1",
    "@payloadcms/richtext-lexical": "3.61.1",
    "@payloadcms/ui": "3.61.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "dotenv": "17.2.3",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "lucide-react": "^0.548.0",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "payload": "3.61.1",
    "prism-react-renderer": "^2.3.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.65.0",
    "sharp": "0.34.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "1.56.1",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "16.0.1",
    "husky": "^9.1.7",
    "jsdom": "27.0.1",
    "lint-staged": "^16.2.6",
    "mongodb": "^6.20.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.20.0+sha256.47a3352808501b8d1ef20112273b6a5dcfa53d28a55bcce36d268e878bd6bfe9"
}
